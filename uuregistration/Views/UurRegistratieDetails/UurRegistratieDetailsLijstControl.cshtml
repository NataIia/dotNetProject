@model IEnumerable<uuregistration.Models.UurRegistratieDetails>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StartTijd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EindTijd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EindDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeWerk)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TeFactureren)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StartTijd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EindTijd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EindDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TypeWerk)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TeFactureren)
        </td>
        <td>
            @Html.ActionLink("Edit", "../UurRegistratieDetails/Edit", new { id=item.UurRegistratieDetailsId }) |
            @Html.ActionLink("Details", "../UurRegistratieDetails/Details", new { id=item.UurRegistratieDetailsId }) |
            @Html.ActionLink("Delete", "../UurRegistratieDetails/Delete", new { id=item.UurRegistratieDetailsId })
        </td>
    </tr>
}

</table>
